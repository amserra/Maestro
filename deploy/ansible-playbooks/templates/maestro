server {
    server_name maestro.ml;
    location = /favicon.ico { access_log off; log_not_found off; }

    location / {
        include proxy_params;
        proxy_pass http://unix:/run/gunicorn/gunicorn.sock;
    }

    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/maestro.ml/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/maestro.ml/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot

}

server {
    server_name www.maestro.ml;
    return 301 $scheme://maestro.ml$request_uri;


    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/maestro.ml/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/maestro.ml/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot

}server {
    if ($host = maestro.ml) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    listen 80;
    server_name maestro.ml;
    return 404; # managed by Certbot


}

server {
    if ($host = www.maestro.ml) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    server_name www.maestro.ml;
    listen 80;
    return 404; # managed by Certbot


}